<p id="notice"><%= notice %></p>

<h1>Listing Buys</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Cantidad</th>
      <th>Proveedor</th>
      <th>Total</th>
      <th>Id producto</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buys.each do |buy| %>
      <tr>
        <td><%= buy.Fecha %></td>
        <td><%= buy.cantidad %></td>
        <td><%= buy.proveedor %></td>
        <td><%= buy.total %></td>
        <td><%= buy.id_producto %></td>
        <td><%= buy.descripcion %></td>
        <td><%= link_to 'Show', buy %></td>
        <td><%= link_to 'Edit', edit_buy_path(buy) %></td>
        <td><%= link_to 'Destroy', buy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Buy', new_buy_path %>
