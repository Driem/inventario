<p id="notice"><%= notice %></p>

<h1>Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Genero</th>
      <th>Telefono</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.nombre %></td>
        <td><%= client.genero %></td>
        <td><%= client.telefono %></td>
        
        <td><%= link_to '  Show', client %></td>
        <td><%= link_to '  Edit', edit_client_path(client) %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Agregar cliente', new_client_path %>
